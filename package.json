{
  "name": "inspect-f",
  "version": "1.1.1",
  "description": "Cast a function to string and adjust its indentation",
  "main": "inspect-f.js",
  "scripts": {
    "clean": "rimraf npm-debug.log coverage",
    "lint": "eslint inspect-f.js test",
    "lint:fix": "eslint --fix inspect-f.js test",
    "postmerge": "npm install",
    "postcheckout": "npm install",
    "precommit": "npm test",
    "release": "npm outdated --long && xyz --edit --repo git@github.com:Avaq/inspect-f.git --tag 'X.Y.Z' --increment",
    "test": "npm run clean && npm run lint && npm run test:unit",
    "test:unit": "istanbul cover --report html _mocha -- --ui bdd --reporter spec --check-leaks --full-trace && codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Avaq/inspect-f.git"
  },
  "keywords": [
    "cast",
    "inspect",
    "function",
    "toString",
    "show",
    "indent"
  ],
  "author": "Aldwin Vlasblom <aldwin.vlasblom@gmail.com> (https://github.com/Avaq)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Avaq/inspect-f/issues"
  },
  "homepage": "https://github.com/Avaq/inspect-f",
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "eslint": "^3.12.2",
    "eslint-config-warp": "^1.2.0",
    "husky": "^0.12.0",
    "istanbul": "^0.4.2",
    "jsverify": "^0.7.1",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.2",
    "xyz": "^2.0.1"
  }
}
